<form method="post" action="{{ path('app_contest_delete', contest.rp ) }}" onsubmit="return confirm('Are you sure you want to delete this contest ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contest.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
